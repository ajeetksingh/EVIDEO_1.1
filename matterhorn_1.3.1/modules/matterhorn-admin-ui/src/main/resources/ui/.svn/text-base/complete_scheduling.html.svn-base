<div id="stage" class="layout-centered form-box">
  <div id="heading-metadata" class="ui-widget-header" style="font-weight:bold; margin-bottom:1em; margin-left:auto; margin-right:auto; padding:10px; text-align:center;">
    A Recording with the following information has been scheduled:
  </div>
  <div id="content-metadata" class="fl-widget-content">
    <ul class="no-bullets formField-list">
      <!-- field: Title -->
      <li id="field-title" class="ui-helper-clearfix" style="display:none; margin-bottom:1em;">
        <span class="formField-label">Title:</span>
        <span class="fieldValue"></span>
      </li>
      <!-- field: Presenter -->
      <li id="field-creator" class="ui-helper-clearfix" style="display:none; margin-bottom:1em;">
        <span class="formField-label">Presenter:</span>
        <span class="fieldValue"></span>
      </li>
      <!-- field: Department -->
      <li id="field-contributor" class="ui-helper-clearfix" style="display:none; margin-bottom:1em;">
        <span class="formField-label">Contributor:</span>
        <span class="fieldValue"></span>
      </li>
      <!-- field: Series -->
      <li id="field-isPartOf" class="ui-helper-clearfix" style="display:none; margin-bottom:1em;">
        <span class="formField-label">Series:</span>
        <span class="fieldValue"></span>
      </li>
      <!-- field: Subject -->
      <li id="field-subject" class="ui-helper-clearfix" style="display:none; margin-bottom:1em;">
        <span class="formField-label">Subject:</span>
        <span class="fieldValue"></span>
      </li>
      <!-- field: Language -->
      <li id="field-language" class="ui-helper-clearfix" style="display:none; margin-bottom:1em;">
        <span class="formField-label">Language:</span>
        <span class="fieldValue"></span>
      </li>
      <!-- field: Description -->
      <li id="field-description" class="ui-helper-clearfix" style="display:none; margin-bottom:1em;">
        <span class="formField-label">Description:</span>
        <span class="fieldValue" style="display:table-cell"></span>
      </li>
      <!-- field: Distribution -->
      <li id="field-distribution" class="ui-helper-clearfix" style="display:none; margin-bottom:1em;">
        <span class="formField-label">Distribution Channel(s):</span>
        <span class="fieldValue"></span>
      </li>
      <!-- field: License -->
      <li id="field-license" class="ui-helper-clearfix" style="display:none; margin-bottom:1em;">
        <span class="formField-label">License:</span>
        <span class="fieldValue"></span>
      </li>
      <!-- field: Capture Agent start time -->
      <li id="field-recordStart" class="ui-helper-clearfix" style="display:none; margin-bottom:1em;">
        <span class="formField-label">Capture start time:</span>
        <span class="fieldValue"></span>
      </li>
      <!-- field: Capture Agent record time -->
      <li id="field-recordTime" class="ui-helper-clearfix" style="display:none; margin-bottom:1em;">
        <span class="formField-label">Capture duration:</span>
        <span class="fieldValue"></span>
      </li>
      <!-- field: Capture Agent -->
      <li id="field-device" class="ui-helper-clearfix" style="display:none; margin-bottom:1em;">
        <span class="formField-label">Capture Agent:</span>
        <span class="fieldValue"></span>
      </li>
      <!-- field: Capture Agent resources-->
      <li id="field-resources" class="ui-helper-clearfix" style="display:none; margin-bottom:1em;">
        <span class="formField-label">Input(s):</span>
        <span class="fieldValue"></span>
      </li>
