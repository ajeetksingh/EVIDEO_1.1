<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
  <head>
    <title id="i18n_window_title"></title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- styles -->
    <link rel="stylesheet" type="text/css" href="css/confidence.css">
    <!-- scripts -->
    <script type="text/javascript" src="jquery/jquery-1.4.2.min.js"></script>
    <script type="text/javascript" src="http://code.jquery.com/jquery.min.js"></script>
    <script type="text/javascript" src="js/confidence.js"></script>
    <script type="text/javascript">
      $(document).ready(Monitor.init);
    </script>
  </head>
  <body>
    <div id="pageHeader">
      <div style="text-align: right; float: right;"><a href="/j_spring_security_logout" style="color:#FFFFFF">Log out</a></div>
    </div>
    <!--div id="pageHeader">
      <div id="currentVersion"></div>
      <div id="newestVersion" style="text-align: right; float: right;"></div>
    </div-->
    <div id="container">
      <div id="page_tab">
      </div>
      <div id="content_area">      
        <div id="video_preview">
          <div>
            <ul id="video_devices" class="device_tab"></ul>
          </div>
          <div id="preview_area">
            <img height="480" width="720" src="img/plug.png" id="snapshot">
          </div>
        </div>
        <div id="audio_preview">
          <div>
            <ul id="audio_devices" class="device_tab"></ul>
          </div>
          <div id="preview_area">
              <div id="left_channel" class="audio_meter">
                <img id="left_mask" class="meter_mask" src="img/mask.png">
              </div>
              <div id="dbValue" style="position: relative;"></div>
              <!-- <div id="right_channel" class="audio_meter" style="display: none;"><img id="right_mask" class="meter_mask" src="img/mask.png"></img></div> -->
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
